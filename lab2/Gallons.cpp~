/* Program: roots.cpp */

#include <iostream>
#include <cmath>

using namespace std;

int main()
{ 
  double a=0, b=0, c=0;
  cout  << "This program calculates the distance a car will go without refilling"  << endl;
  cout  << "of the form a * b = c " << endl << endl;
  cout  << "Enter A and B:";
  cin >> a >> b;
  double = (a*b)=c;
  cout << c;
  return 0;
}


